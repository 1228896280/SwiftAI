  pod 'SnapKit'
  # Pods for SwiftAI

  target 'SwiftAITests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SwiftAIUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
